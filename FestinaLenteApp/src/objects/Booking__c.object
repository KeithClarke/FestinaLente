        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/26.0/connection.js&apos;)}
{!REQUIRESCRIPT(&apos;/soap/ajax/26.0/apex.js&apos;)}

var ids = {!GETRECORDIDS( $ObjectType.Booking__c)};
if (ids.length &gt; 0) {
    if (confirm(&apos;Are you sure you want to delete the selected Booking(s)?&apos;)) {
        sforce.apex.execute(&apos;Actions&apos;, &apos;deleteBookings&apos;, {ids: ids});
        window.location.reload();
    }
} else {
    alert(&apos;Select at least one Booking first using the checkboxes.&apos;);
}</url>
